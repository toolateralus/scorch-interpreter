
struct Vec3 |
    x := 0.0
    y := 0.0
    z := 0.0
|

var arrays := []

println('arrays len : ', len(arrays))

const start := time()

repeat z < 1000 {
    repeat i < 500 {
        arrays = push(arrays, push([], new Vec3 { 0.0, 0.0, 0.0 }))
    }
}

const end := time()

const delta := end - start

delta = floor(delta)

println('time spent: ', delta, 'ms')

println('arrays len : ', len(arrays))