struct Vec2 |
	var x : double
	var y : double
|

within Vec2  {
	add : Vec2(this : Vec2, other : Vec2) {
		x := this.x + other.x
		y := this.y + other.y
		return new Vec2(x, y)
	}
}

struct Player |
	var position := new Vec2()
	var scale := new Vec2()
|

struct Scene |
	objects : array = [].push(new Player())
|

const scene := new Scene()

